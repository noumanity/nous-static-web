<!DOCTYPE html>
<html lang="en" class="dark">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Noumanity</title>
    <link rel="stylesheet" href="/assets/css/styles.css">
</head>

<body class="font-body">


<div class="bg-cover bg-no-repeat h-auto bg-hero-tablet lg:bg-hero-desktop">
    <nav id="nav-bar-noumanity" class="bg-transparent py-6 relative">
        <!--<div class="container mx-auto flex">-->
        <div class="container mx-auto grid grid-cols-3 md:grid-cols-4 lg:grid-cols-5 gap-4">
            <!--<div class="flex flex-grow">-->
            <div class="col-span-2 md:10 lg:pl-20">
                <img class="hidden md:block lg:block md:w-80 lg:w-96" src="/assets/img/logo noumanity-orange.png"
                    alt="">
                <!--MOBILE-->
                <img class="block w-28 md:hidden lg:hidden md:w-80 lg:w-96"
                    src="/assets/img/logo noumanity-white.png" alt="">
                <!--END MOBILE-->
            </div>
            <!--MOBILE-->
            <div class=" block md:hidden lg:hidden grid grid-cols-3 items-center">
                <div class="col-end-auto">
                    <svg xmlcol-span-4 ns="http://www.w3.org/2000/svg" class="stroke-0 text-white" fill="none"
                        viewBox="0 0 24 24" stroke="currentColor" stroke-width="2">
                        <path stroke-linecap="round" stroke-linejoin="round" d="M4 6h16M4 12h16M4 18h16" />
                    </svg>
                </div>
            </div>
            <!--END MOBILE-->
            <!--Menú-->
            <div class="hidden md:block lg:block">
                <div class=" pt-8 text-right md:grid grid-cols-3">
                    <% for(var i=0; i<contenido.data.menus.length; i++) {%>
                        <a href="#" class="text-sm text-<%=diseno.claro.color.menu.enlace.blur%> hover:text-<%=diseno.claro.color.menu.enlace.hover%>-600 dark:hover:text-<%=diseno.oscuro.color.menu.enlace.hover%>-600 pr-3"><%=contenido.data.menus[i].menu.enlace%></a>
                    <% } %>
                </div>
            </div>
            <div class="hidden md:block lg:block pt-8 md:pl-10 lg:pl-40 text-center">
                <% for(var i=0; i<contenido.data.idiomas.length; i++) {%>
                    <a href="#" class="text-sm text-<%=diseno.claro.color.menu.enlace.blur%> hover:text-<%=diseno.claro.color.menu.enlace.hover%>-600 dark:text-<%=diseno.oscuro.color.menu.enlace.blur%> dark:hover:text-<%=diseno.oscuro.color.menu.enlace.hover%>-600 pr-2 hover:font-bold"><%=contenido.data.idiomas[i].idioma.nombre%></a>
                <% } %>
            </div>
            <!--End Menú-->
        </div>
    </nav>
    <!--Banner-->
    <div name="banner"
        class="py-20 grid grid-cols-1 justify-center items-center md:py-96 md:pt-80 md:pb-32 md:flex md:flex-col md:justify-end md:items-end lg:py-96 lg:flex lg:flex-col lg:justify-end lg:items-end">
        <p
            class="text-<%=diseno.claro.color.banner.titulo%> dark:text-<%=diseno.oscuro.color.banner.titulo%> text-2xl font-bold px-20 md:text-right md:text-5xl lg:text-5xl md:font-bold md:px-44 lg:text-right lg:text-5xl lg:font-bold lg:px-44">
            <%=contenido.data.banner.titulo%>
    </div>
    <!--End Banner-->
    <!--Menú Icono-->
    <div class="grid grid-cols-4 hidden md:block lg:block">
        <div
            class="col-start-2 col-span-4 text-white hover:text-<%=diseno.claro.color.menu.enlace.hover%>-600 text-5xl flex flex-col justify-center items-center">
            <svg xmlns="http://www.w3.org/2000/svg" class="h-20 w-20" fill="none" viewBox="0 0 24 24"
                stroke="currentColor" stroke-width="2">
                <path stroke-linecap="round" stroke-linejoin="round"
                    d="M15 13l-3 3m0 0l-3-3m3 3V8m0 13a9 9 0 110-18 9 9 0 010 18z" />
            </svg>
        </div>
    </div>
    <!--End Menú Icono-->
</div>

<!--Primera sección-->
<section name="primera_seccion">
